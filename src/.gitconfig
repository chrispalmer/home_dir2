[color]
	grep = auto
	diff = auto
	status = auto
	ui = auto
	branch = auto
[color "grep"]
	filename = magenta
	linenumber = green
[color "diff"]
	func = blue
[color "status"]
	untracked = yellow
	branch = green
[core]
	whitespace = trailing-space,space-before-tab
	excludesfile = /home/<set_me}>/.gitignore
	editor = sublime -n -w
[grep]
	lineNumber = true
[push]
	default = simple
[rebase]
	autosquash = true
[rerere]
	enabled = true
[pager]
	log = less -E
[user]
	name = <set_me>
	email = <set_me>
