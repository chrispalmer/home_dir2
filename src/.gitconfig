[color]
	grep = auto
	diff = auto
	status = auto
	ui = auto
	branch = auto
[color "grep"]
	filename = magenta
	linenumber = green
[color "diff"]
	func = blue
[color "status"]
	untracked = yellow
	branch = green
[core]
	whitespace = trailing-space,space-before-tab
	excludesfile = /Users/cpalmer/.gitignore_global
	editor = nano
[grep]
	lineNumber = true
[push]
	default = tracking
[rebase]
	autosquash = true
[rerere]
	enabled = true
[pager]
	log = less -E
[user]
	name = "Chris Palmer"
	email = chris@crpalmer.com
[init]
	templatedir = ~/git/templates
